{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/page.jsx", "src/components/HomePage/Nurture/NurtureKey.jsx", "src/app/about-mejoria/page.jsx", "src/app/contact-us/page.jsx", "src/app/our-expertise/page.jsx", "src/app/eggfree-cake-diploma-baking-course/page.jsx", "src/app/cake-mastery-baking-certification-course/page.jsx"],
  "exclude": ["node_modules"]
}
